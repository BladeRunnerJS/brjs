            "sdk/libs/javascript/jasmine",
            "sdk/libs/javascript/jquery",
            "sdk/libs/javascript/jqueryplugins",
            "sdk/libs/javascript/jshamcrest",
            "sdk/libs/javascript/jsmockito",
            "sdk/libs/javascript/jstd",
            "sdk/libs/javascript/jstestdriverextensions",
            "sdk/libs/javascript/jsunitextensions",
            "sdk/libs/javascript/ko",
            "sdk/libs/javascript/log4js",
            "sdk/libs/javascript/mock4js",
            "sdk/libs/javascript/momentjs",
            "sdk/libs/javascript/openajax",
            "sdk/libs/javascript/zeroclipboard",
            "sdk/libs/javascript/fell",
            "sdk/libs/javascript/topiarist",
